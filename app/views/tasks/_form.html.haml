= simple_form_for(@task) do |f|
  = f.error_notification

  .form-inputs
    = f.association :my_process
    = f.association :user
    = f.association :responsible_user
    = f.input :name
    = f.input :description
    = f.input :address
    = f.input :latitude
    = f.input :longitude
    = f.input :starts_at
    = f.input :ends_at
    
  .form-actions
    = f.button :submit
